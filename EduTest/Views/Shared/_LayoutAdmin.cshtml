
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <title>@*@ViewBag.Title*@</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <link href="~/Tookit/layui/css/layui.css" rel="stylesheet" />
    <link href="~/Tookit/inspinia_admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Tookit/inspinia_admin/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/Tookit/inspinia_admin/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="~/Tookit/inspinia_admin/css/animate.css" rel="stylesheet">
    <link href="~/Tookit/inspinia_admin/css/style.css" rel="stylesheet">

    <style type="text/css">
        .page-heading {
            padding: 10px;
        }

        .minimalize-styl-2 {
            margin: 0px;
        }
    </style>
    @RenderSection("Css", false)

    <!-- Mainly scripts -->
    <script src="~/Tookit/inspinia_admin/js/jquery-2.1.1.js"></script>
    <script src="~/Tookit/inspinia_admin/js/bootstrap.min.js"></script>
    <script src="~/Tookit/inspinia_admin/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/Tookit/inspinia_admin/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Tookit/bootstrap/js/bootstrap3-validation.js"></script>
    <!-- Peity -->
    <script src="~/Tookit/inspinia_admin/js/plugins/peity/jquery.peity.min.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="~/Tookit/inspinia_admin/js/inspinia.js"></script>


    <script src="~/Scripts/jquery.form.js"></script>
    <script src="~/Scripts/JsWindows.js"></script>
    <script src="~/Scripts/JsCommon.js"></script>
    <script src="~/Tookit/layui/layui.js"></script>
    <script type="text/javascript">
        layui.use('layer');
    </script>
</head>
<body>
    @RenderBody()
    @RenderSection("Js", false)
</body>
</html>
